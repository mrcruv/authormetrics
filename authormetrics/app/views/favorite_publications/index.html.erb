<p style="color: green"><%= notice %></p>

<div class='margine'>
<h1>Favorite publications</h1>
<div id="favorite_publications">
  <% @favorite_publications.each do |favorite_publication| %>
    <%= render favorite_publication %>
    <%= link_to "Show", publication_path(favorite_publication.publication_id) %>
    <%= button_to 'Destroy', [User.find(favorite_publication.user_id),favorite_publication ], :method=>:delete %>
    <br>
    <%="--------------------------------------"%>
  <% end %>
  
</div>

<%= link_to "New favorite publication", new_user_favorite_publication_path %>
</div>
